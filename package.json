{
    "name": "Kamesennin",
    "version": "0.1.0",
    "description": "Discord bot for keeping Jontka occupied.",
    "main": "index.js",
    "author": "Ionisaatio",
    "scripts": {
        "babel-node": "babel-node --presets=env",
        "k-start": "nodemon --exec babel-node src/index.js",
        "k-build": "babel src -d dist",
        "k-test": "echo \"Error: no test specified\" && exit 1",
        "clean": "rm -rf build && mkdir build",
        "build-babel": "babel -d ./build ./src -s",
        "build": "npm run clean && npm run build-babel",
        "start": "npm run build && node ./build/index.js"
    },
    "dependencies": {
        "@babel/cli": "^7.0.0",
        "discord.io": "https://github.com/woor/discord.io/tarball/gateway_v6",
        "dotenv": "^7.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.3.4",
        "@babel/node": "^7.0.0",
        "@babel/preset-env": "^7.3.4",
        "nodemon": "^1.18.10"
    }
}
